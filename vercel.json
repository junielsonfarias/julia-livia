{
  "version": 2,
  "name": "julia-livia",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/big-hero",
      "destination": "/big-hero-game/index.html"
    },
    {
      "source": "/tabuada",
      "destination": "/tabuada-game/index.html"
    },
    {
      "source": "/memoria",
      "destination": "/memoria-game/index.html"
    },
    {
      "source": "/cores-formas",
      "destination": "/cores-formas-game/index.html"
    },
    {
      "source": "/sequencia",
      "destination": "/sequencia-game/index.html"
    },
    {
      "source": "/quiz",
      "destination": "/quiz-game/index.html"
    },
    {
      "source": "/desenhar",
      "destination": "/desenhar-game/index.html"
    },
    {
      "source": "/contar",
      "destination": "/contar-game/index.html"
    },
    {
      "source": "/animais",
      "destination": "/animais-game/index.html"
    },
    {
      "source": "/puzzle",
      "destination": "/puzzle-game/index.html"
    }
  ]
}
